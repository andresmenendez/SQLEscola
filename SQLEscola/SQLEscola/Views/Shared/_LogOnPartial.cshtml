@if(Request.IsAuthenticated) {
    <text>Perfil: @Session["Perfil"]</text>
    <br />
    <text>Usuário: <strong>@User.Identity.Name</strong>!
    [ @Html.ActionLink("Sair", "LogOff", "Account") ]</text>
}
else {
    @:[ @Html.ActionLink("Registrar-se", "Register", "Account") ]
    @:[ @Html.ActionLink("Entrar", "LogOn", "Account") ]
}
