@model SQLEscola.Models.TurmaModel

@{
    ViewBag.Title = "Remover";
}

<h2>Remover</h2>

<h3>Tem certeza que deseja remover esta turma?</h3>
<fieldset>
    <legend>Turma</legend>

    <div class="display-label">Nome da Turma</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Turma)
    </div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <p>
            <input type="submit" value="Remover" class="but but-error but-small but-shadow but-rc" />
        </p>
        @Html.ActionLink("Voltar", "FiltrarPorUser")
    </p>
}
