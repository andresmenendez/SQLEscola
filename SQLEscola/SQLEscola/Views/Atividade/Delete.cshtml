@model SQLEscola.Models.AtividadeModel

@{
    ViewBag.Title = "Remover";
}

<h2>Remover</h2>

<h3>Tem certeza que deseja remover esta atividade?</h3>
<fieldset>
    <legend>Atividade</legend>

    <div class="display-label">Nome da Atividade</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Nome_Atividade)
    </div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <p>
            <input type="submit" value="Remover" class="but but-error but-small but-shadow but-rc" />
        </p>
        @Html.ActionLink("Voltar", "Index")
    </p>
}

