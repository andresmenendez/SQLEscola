@model IEnumerable<SQLEscola.Banco.tb_resultado>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Erros
        </th>
        <th>
            Comentarios_Prof
        </th>
        <th>
            tb_resposta
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Erros)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Comentarios_Prof)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tb_resposta.ScriptResposta)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id_Resultado }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id_Resultado }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id_Resultado })
        </td>
    </tr>
}

</table>
